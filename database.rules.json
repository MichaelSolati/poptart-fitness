{
	"rules": {
		".read": "true",
		".write": "false",
		"events": {
			".write": "auth != null",
			".indexOn": [ "starts", "placeId", "uid" ]
		},
		"activeEvents": {
			".indexOn": [ "starts", "hash", "placeId" ]
		},
		"places": {
			".indexOn": "hash"
		},
		"profiles": {
			".indexOn": "email"
		},
		"userBadges": {
			".indexOn": "uid"
		}
	}
}